#include<stdio.h>
main() 
{
int choice;
printf("FOOD CHOICE :\n 1. Pizza - Rs 239\n 2. Burger - Rs 129\n 3. Pasta - Rs 179\n 4. Frennch Fries - Rs 99\n 5. Sandwich - Rs 149\n");
printf("Enter your choice :");
scanf("%d", &choice);
switch (choice) 
{
case 1: printf("Food item - Pizza\n Price - Rs 239");
break;
case 2: printf("Food item - Burger\n Price - Rs 129");
break;
case 3: printf("Food item - Pasta\n Price - Rs 179");
break;
case 4: printf("Food item - Fries\n Price - Rs 99");
break;
case 5: printf("Food item - Sandwich\n Price - Rs 149");
break;
default:
printf("Enter a valid number between 1 and 5");
}
return 0;
}
    
